{
  "name": "igncp-vscode-extension",
  "displayName": "igncp-vscode-extension",
  "private": true,
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "igncp-vscode-extension.easyLog",
        "title": "Easy Log"
      },
      {
        "command": "igncp-vscode-extension.copyFileDirPath",
        "title": "Copy File Dir Path"
      },
      {
        "command": "igncp-vscode-extension.copyFileRelativePathMonorepo",
        "title": "Copy File Relative Path Monorepo"
      },
      {
        "command": "igncp-vscode-extension.listFilesInDirOrBelow",
        "title": "List Files In Dir Or Below"
      }
    ]
  },
  "scripts": {
    "fix": "eslint --fix 'src/**/*.ts' && prettier --write 'src/**/*.ts'",
    "package": "bun run package:test && bun run package:content",
    "package:content": "webpack --mode production --devtool hidden-source-map && vsce package --allow-missing-repository",
    "package:test": "prettier --write 'src/**/*.ts'",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@igncp/common-config": "0.0.11",
    "@types/node": "22.x",
    "@types/vscode": "^1.50.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "ts-loader": "^9.5.4",
    "vsce": "^2.15.0",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1"
  }
}